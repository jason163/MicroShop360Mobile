  transform: scale(.5);
  -webkit-transform-origin: 0 0;
  -moz-transform-origin: 0 0;
  -ms-transform-origin: 0 0;
  -o-transform-origin: 0 0;
  transform-origin: 0 0;
  pointer-events: none;
  -webkit-border-radius: 16px;
  -moz-border-radius: 16px;
  border-radius: 16px;
  border: 1px solid #ddd; 
  overflow: hidden;  
}
#white_header i.clearText{ background: url(../img/ico_cleartext.png) no-repeat center center; color: #fff; display: none; font-family: arial; height: 24px; margin-top: -12px; overflow: hidden; position: absolute;right: 4px;top:50%; width: 24px; z-index: 2;
-moz-opacity: 0.15;
opacity: 0.15;
-webkit-background-size: 16px auto;
background-size: 16px auto;
}
#white_header i.clearText.on{ display: block;}
/*白底共用头部 end*/
.return{width: 32px; height: 32px; overflow: hidden;display: block; position:absolute; float: left; left: 0;}
.return:after{ border-bottom: solid 1px #999; border-left: solid 1px #999; content: ""; display: inline-block; height: 13px; vertical-align: 1px; width: 13px;top: 10px; left: 10px; position: absolute;
-webkit-transform: rotate(45deg);
-moz-transform: rotate(45deg);
-ms-transform: rotate(45deg);
-o-transform: rotate(45deg);
transform: rotate(45deg);
}
.right_link{ height: 32px; line-height: 34px; position:absolute; float: right; right: 10px;top: 0; overflow: hidden;}
.headerplace{ height: 44px; width: 100%; }

/* 带有彩色数线的标题栏 */
.hd_withline{ background-color: #fff; border-left: solid 3px #D90B0B; height: 34px; line-height: 34px; padding-left: 10px; padding-right: 10px; width: 100%;}
.hd_withline .layout{ height: 100%; position: relative; width: 100%;}
.hd_withline h3{ color: #D90B0B; display: block; font-size: 15px; height: 100%; vertical-align: middle;}
.hd_withline a{ color: #999; font-size: 12px; height: 100%; position: absolute;right:0;top:0;}
.hd_withline a:after{ border-top: solid 1px #999; border-right: solid 1px #999; content: ""; display: inline-block; height: 6px; vertical-align: 1px; width: 6px;
-webkit-transform: rotate(45deg);
-moz-transform: rotate(45deg);
-ms-transform: rotate(45deg);
-o-transform: rotate(45deg);
transform: rotate(45deg);
}

/*按钮*/
/**红线红字个人中心用按钮**/
.c_item_opera{padding: 5px 10px;}
.c_item_opera .bntlinered{float: right; margin-left: 5px;}
.bntlinered{ padding:0 10px; line-height: 30px; height:28px; overflow:hidden; background: #fff; color: #f53636 !important; display: inline-block; position: relative; }
.bntlinered:before{  
  position: absolute;
  content: "";
  display: block;
  left: 0;
  top: 0;
  right: -100%;
  bottom: -100%;
  -webkit-transform: scale(.5);
  -moz-transform: scale(.5);
  -ms-transform: scale(.5);
  -o-transform: scale(.5);
  transform: scale(.5);
  -webkit-transform-origin: 0 0;
  -moz-transform-origin: 0 0;
  -ms-transform-origin: 0 0;
  -o-transform-origin: 0 0;
  transform-origin: 0 0;
  pointer-events: none;
  -webkit-border-radius: 8px;
  -moz-border-radius: 8px;
  border-radius: 8px;
  border: 1px solid #f53636; 
}
/***红色背景按钮**/
.btnredbg,
a.btnredbg{ background-color: #D90B0B; color: #fff; display: inline-block; font-size: 19px; height: 44px; line-height: 44px; width: 100%;
-webkit-border-radius: 4px;
-moz-border-radius: 4px;
border-radius: 4px; text-align: center;}
.graybg,
a.graybg{ background-color: #aaa!important;}

/**banner切换样式**/
.sliding{ position: relative; overflow: hidden;}
.sliding .img{width:100%; height: auto; overflow: hidden; background-position: center;  display: none; float: left; background-repeat:no-repeat; 
-webkit-background-size: 100% auto;
background-size: 100% auto;}
.sliding .img.current{ display: block;}
.sliding ul.sd{ text-align: center; position: absolute;bottom:0;left: 0;right: 0;margin:auto;overflow: hidden; padding: 0;} 
.sliding ul.sd li{ background:rgba(0,0,0,0.2); width:10px; height: 10px; border-radius: 50%; display: inline-block; margin: 0 2px;}
.sliding ul.sd li.on{background:rgba(0,0,0,0.5);}

/*价格公用*/
.price{ line-height: 22px; height: 20px;}
.price span{ color: #D90B0B; font-size: 16px;}
.price span:before{content: "￥"; font-size: 12px;}
.price del{ color: #999; font-size: 12px;}


