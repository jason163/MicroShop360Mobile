-moz-transform: scaleX(.5);
-ms-transform: scaleX(.5);
-o-transform: scaleX(.5);
transform: scaleX(.5);
}
.global_ad .col:after{ background-color: #fff; height: 1px; content: ""; position: absolute;left: 0;top: 0; width: 100%;
