/* 横向背景分隔线 */
.box-line-t,
.box-line-b{ position: relative;}
.box-line-t:before,
.box-line-b:after{ background-color: #ccc; content: ""; height: 1px; position: absolute;left: 0; width: 100%;
-webkit-transform: scaleY(.5);
-moz-transform: scaleY(.5);
-ms-transform: scaleY(.5);
-o-transform: scaleY(.5);
transform: scaleY(.5);
}
.box-line-t:before{ top: 0;}
.box-line-b:after{ bottom: 0;}

/* 横向背景分隔线2 */
.box-line-t2,
.box-line-b2{ padding-top: 1px; padding-bottom: 1px; position: relative;}
.box-line-t2:before,
.box-line-b2:after{ background-color: #ddd; content: ""; height: 1px; position: absolute;left: 0; width: 100%;
-webkit-transform: scaleY(.5);
-moz-transform: scaleY(.5);
-ms-transform: scaleY(.5);
-o-transform: scaleY(.5);
transform: scaleY(.5);
}
.box-line-t2:before{ top: 0;}
.box-line-b2:after{ bottom: 0;}

/*border*/
.box-border{position: relative; display: inline-block;}
.box-border:after{	
  position: absolute;
  content: "";
  display: block;
  left: 0;
  top: 0;
  right: -100%;
  bottom: -100%;
  -webkit-transform: scale(.5);
  -moz-transform: scale(.5);
  -ms-transform: scale(.5);
  -o-transform: scale(.5);
  transform: scale(.5);
  -webkit-transform-origin: 0 0;
  -moz-transform-origin: 0 0;
  -ms-transform-origin: 0 0;
  -o-transform-origin: 0 0;
  transform-origin: 0 0;
  pointer-events: none;
  border: 1px solid #ddd; 	
}

header {
	background: #e10005;
	height: 44px;
	position: fixed;
	left: 0;
	top: 0;
	z-index: 99999;
}

header .headerLeft {
	float: left;
	height: 20px;
	position: relative;
	top: 12px;
	z-index: 2;
}

header a.back {
	padding: 12px 3px 12px 12px;
	position: relative;
	float: left;
	margin-right: 4px;
	height: 20px;
	width: 20px;
	z-index: 2;
	-webkit-background-size: 20px auto;
	background-size: 20px auto;
}

header a.back:before {
	border-left: solid 2px #ffffff;
	border-bottom: solid 2px #ffffff;
	content: "";
	display: inline-block;
	height: 10px;
	width: 10px;
	-webkit-transform: rotate(45deg) translate( 5px, -5px);
	-moz-transform: rotate(45deg) translate( 5px, -5px);
	-ms-transform: rotate(45deg) translate( 5px, -5px);
	-o-transform: rotate(45deg) translate( 5px, -5px);
	transform: rotate(45deg) translate( 5px, -5px);
}

header .headerLeft a.logoBox {
	background: url(/Resources/Nest/img/logo.png) no-repeat center center;
	position: relative;
	float: left;
	height: 20px;
	width: 64px;
	-webkit-background-size: 64px auto;
	background-size: 64px auto;
}

header .headerBox {
	position: absolute;
	left: 0;
	top: 8px;
	width: 100%;
	z-index: 1;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}

.headerBox h1 {
	color: #fff;
	font-weight: 700;
	vertical-align: middle;
	text-align: center;
	position: relative;
	font-size: 18px;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}



/* 底部 */

footer {
	background-color: #fff;
	height: 50px;
	position: fixed!important;
	left: 0;
	bottom: 0;
	width: 100%;
	z-index: 101;
	-webkit-box-shadow: 0 -1px 5px rgba(0, 0, 0, .15);
	-moz-box-shadow: 0 -1px 5px rgba(0, 0, 0, .15);
	box-shadow: 0 -1px 5px rgba(0, 0, 0, .15);
}

footer ul {
	display: block;
	height: 50px;
	width: 100%;
}

footer li {
	float: left;
	height: 50px;
	position: relative;
	text-align: center;
	width: 25%;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}

footer li a {
	background: no-repeat center;
	display: inline-block;
	height: 100%;
	padding-top: 5px;
	width: 50px;
	-webkit-background-size: 24px auto;
	background-size: 24px auto;
}
footer li a.a1{ background-image: url(../img/ico_home.png);}
footer li a.a1.on{ background-image: url(../img/ico_home_on.png);}
footer li a.a2{ background-image: url(../img/ico_search.png);}
footer li a.a2.on{ background-image: url(../img/ico_search_on.png);}
footer li a.a3{ background-image: url(../img/ico_cart.png);}
footer li a.a3.on{ background-image: url(../img/ico_cart_on.png);}
footer li a.a4{ background-image: url(../img/ico_member.png);}
footer li a.a4.on{ background-image: url(../img/ico_member_on.png);}

footer li i {
	background: #f20005;
	color: #fff;
	font-size: 12px;
	height: 16px;
	line-height: 18px;
	overflow: hidden;
	position: absolute;
	left: 50%;
	top: 0;
	min-width: 12px;
	padding: 0 2px;
	margin: 10px 0px 0px 4px;	
	-webkit-border-radius: 16px;
	-moz-border-radius: 16px;
	border-radius: 16px;
}

/* 广告位 */
.global_ad{ background: #fff;}
.global_ad .col{ position: relative;}
.global_ad .col:before{ background-color: #fff; content: ""; height: 100%; position: absolute;left: -1px;top: 0; width: 1px;
