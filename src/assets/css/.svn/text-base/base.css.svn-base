/* 带有图标的标题栏 */
.hd_withIcon{ background-color: #fff; height: 34px; line-height: 34px; padding-left: 10px; padding-right: 10px; width: 100%;}
.hd_withIcon .layout{ height: 100%; position: relative; width: 100%;}
