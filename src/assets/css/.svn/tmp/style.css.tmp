@charset "utf-8";
.layer{ padding-bottom: 50px;}
.name .labelred{ color:#D90B0B; font-size: 12px; overflow: hidden; position: relative; padding: 2px 2px; margin-right: 3px;} 
.name .labelred:after{ 
  position: absolute;
  content: "";
  display: block;
  left: 0;
  top: 0;
  right: -100%;
  bottom: -100%;
  -webkit-transform: scale(.5);
  -moz-transform: scale(.5);
  -ms-transform: scale(.5);
  -o-transform: scale(.5);
  transform: scale(.5);
  -webkit-transform-origin: 0 0;
  -moz-transform-origin: 0 0;
  -ms-transform-origin: 0 0;
  -o-transform-origin: 0 0;
  transform-origin: 0 0;
  pointer-events: none;
  -webkit-border-radius: 8px;
  -moz-border-radius: 8px;
  border-radius: 8px;
  border: 1px solid #D90B0B; 
  overflow: hidden;  
}

/**列表页**/
.list_tit{ background: #fff; position:fixed; z-index: 101;width: 100%; height: 40px; overflow: hidden;}
.list_tit li{ width: 33.333333%; display: inline-block;float: left; text-align: center;}

.list_tit li span{ display: inline-block;padding-right: 18px;background-position:right center; line-height: 42px; height: 40px; overflow: hidden; background-repeat:no-repeat;
-webkit-background-size: 12px 12px;
background-size: 12px 12px;}
.list_tit li .sort{padding-right: 0; }
.list_tit li .sort_down{ background-image: url(../img/ico_down.png);}
.list_tit li.on .sort_down{ background-image: url(../img/ico_down_on.png); color: #D90B0B;}
.list_tit li .sort_up{ background-image: url(../img/ico_up.png);}
.list_tit li.on .sort_up{ background-image: url(../img/ico_up_on.png); color: #D90B0B;}
.list_tit li .sort_sx{ background-image: url(../img/ico_shaixuan.png);}
.list_tit li.on .sort_sx{ background-image: url(../img/ico_shaixuan_on.png); color: #D90B0B;}
.list_con li{ position: relative; display: block; overflow: hidden;padding: 10px 10px; background: #fff;}
.list_con li:before{content: ""; width: 100%; height: 1px; display:block; position: absolute; background-color: #ddd;bottom: 0; left: 0;
  -webkit-transform: scaleY(.5);
  -moz-transform: scaleY(.5);
  -ms-transform: scaleY(.5);
  -o-transform: scaleY(.5);
  transform: scaleY(.5);} 
.list_con li a{display: block; overflow: hidden;clear: both;}
.list_con li .img{ width: 80px; height: 80px; overflow: hidden; float: left; margin-right: 5px;
background-position: center; background-repeat:no-repeat;
-webkit-background-size: 80px auto;
background-size: 80px auto; overflow: hidden;}
.list_con li .name{ max-height: 40px; line-height: 20px; overflow: hidden;}
.list_con li .sales_word{ font-size: 12px; color: #D90B0B;display:block;white-space:nowrap; overflow:hidden; text-overflow:ellipsis; line-height: 22px; height: 20px;}
.list_con li .add_cart{ position: absolute; bottom:10px; right: 10px; width: 22px; height: 20px;  background: url(../img/ico_addcart.png) center center no-repeat; 
-webkit-background-size: 22px auto;
background-size: 22px auto;}
.l_more{ font-size: 12px; color: #999;text-align: center; padding: 5px 0;}
.l_more i{letter-spacing: -2px;}
