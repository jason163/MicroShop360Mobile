/* Left Tab */
.category{ height: 100%; padding-top: 54px;}
.category .catelist { border-top: solid 1px #ddd; position:relative; height: 100%; width: 100%;}

.category .tab { border-right: 1px solid #ddd; width: 88px; display: block; background:#F0F0F0; height: 100%; position: absolute;left:0;top:0;}
.category .tab ul{ height: 100%; overflow-x: hidden; width: 88px;}
.category .tab li{ width: 100%; border-bottom: 1px solid #ddd; display: block;  position: relative; }
.category .tab li a { display: block; padding:16px 0; text-align: center;}
.category .tab li.current { background-color: #fff; position: relative;}
.category .tab li.current a {color: #D90B0B;}
.category .tab li.current:after {content:''; display: block; position: absolute; height: 100%; width:1px; top:0px; right:-1px; background-color:#fff; z-index: 2; }

.category .tabc{ background: #fff; overflow-x: hidden; padding-left: 88px; height: 100%; width: 100%;}
.category .tabc ul{}
.category .tabc li { float: left; width: 33.33333333%; }
.category .tabc li a { display: block; margin:5px; box-sizing: border-box; -webkit-box-sizing: border-box; -ms-box-sizing: border-box;}
.category .tabc li img { display: block; width: 100%;}
.category .tabc li a em { display: block; text-align: center; font-style: normal; font-size: 12px; color: #2c2c2c;}

